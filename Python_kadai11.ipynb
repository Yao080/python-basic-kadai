        "planets = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "#  forを使ってplanetsの要素を1つずつ順番に出力\n",
        "for planet in planets:\n",
        "  print(planet)\n",
        "\n",
        "# whileを使ってplanets の要素を1つずつ順番に出力\n",
        "i = 0\n",
        "while planets[i] != \"冥\":\n",
        "  print(planets[i])\n",
        "  i += 1"
      ]
